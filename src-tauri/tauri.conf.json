{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "licitacija",
  "identifier": "com.licitacija.app",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "licitacija",
        "width": 1400,
        "height": 700,
        "maximized": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "createUpdaterArtifacts": true
  },
  "plugins": {
    "sql": {
      "preload": ["sqlite:main_database_v2.db"]
    },
    "filesystem": {},
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDdGM0Y1MzY2NDgxRjkwMkUKUldRdWtCOUlabE0vZng3aDlBc3F2MDl6ZzBSTDdkaWtxVnRZRjhHbTR4Tk0yc2phTkV5VFc1NVoK",
      "endpoints": [
        "https://github.com/xtrinch/tauri-todo-sql/releases/latest/download/release.json"
      ],
      "windows": {
        "installMode": "passive"
      }
    }
  }
}
